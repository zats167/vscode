}

.content_integral {
    font-size: 24rpx;
}

.content_font {
    font-size: 42rpx;
}

.content_coll {
    font-size: 22rpx;
}

.content_middle {
    color: #ff5f3e;
}

.content_right {
    color: #6ac20b;
}

.logs_order {
    margin: 20rpx 0rpx;
    background-color: white;
    font-size: 28rpx;
    color: #333;
    padding: 0rpx 30rpx;
    border-top: 1rpx solid #f2f2f2;
    border-bottom: 1rpx solid #f2f2f2;
}

.logs_center {
    height: 110rpx;
    border-bottom: 1rpx solid #f2f2f2;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

button.logs_center {
    padding: 0px;
    border-radius: 0px;
    background-color: white;
    font-size: 28rpx;
    color: #333;
}

.logs_center:last-child {
    border-bottom: 0px;
}

.center_img {
    width: 16rpx;
    height: 26rpx;
}

.center_map {
    width: 68rpx;
    height: 68rpx;
    margin-right: 25rpx;
}

.logs_centerimg {
    display: flex;
    align-items: center;
}

.logs_seller {
    margin: 20rpx auto;
    background-color: white;
    font-size: 30rpx;
    color: #333;
    padding: 0rpx 30rpx;
    border-top: 0px solid #f2f2f2;
    border-bottom: 0px solid #f2f2f2;
    border-radius: 0px;
}

.footer2 {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 80rpx;
}

.footer_img {
    width: 50rpx;
    height: 50rpx;
}

.footer_text1 {
    font-size: 36rpx;
    color: #999;
    margin-left: 10rpx;
}

.copyright {
    height: 120rpx;
    padding: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.copyright_logo {
    display: flex;
    align-items: center;
}

.copyright_img {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    border-radius: 6rpx;
}

.copyright_text {
    color: #ccc;
    font-size: 30rpx;
    font-weight: bold;
}

.copyright_txt {
    font-size: 26rpx;
    color: #ccc;
    margin-top: 10rpx;
}

.order-block {
    background: #fff;
    margin-bottom: 20rpx;
    padding: 0 12px;
}

.order-block .my-order {
    font-size: 30rpx;
    border-bottom: 1rpx solid #f2f2f2;
    padding: 32rpx 12rpx 12rpx 12rpx;
}

.order-block .my-order .n {
    width: 100%;
    flex: 1;
}

.order-block .nav-item {
    text-align: center;
    display: block;
    font-size: 9pt;
    padding: 24rpx 0;
    position: relative;
    color: #999;
}

.order-block .nav-item .num-icon {
    position: absolute;
    top: 8rpx;
    left: 50%;
    margin-left: 12rpx;
    display: inline-block;
    background: #f00;
    color: #fff;
    border-radius: 999rpx;
    padding: 0 8rpx;
    min-width: 40rpx;
    font-size: 7pt;
}

.order-block .nav-item image {
    width: 50rpx;
    height: 50rpx;
    margin-bottom: 5rpx;
}

.flex-row {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    flex-direction: row;
}

.flex-col {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    flex-direction: column;
}

.flex-grow-0 {
    min-width: 0;
    -webkit-box-flex: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
    -ms-flex-negative: 0;
    flex-shrink: 0;
}

.flex-grow-1 {
    min-width: 0;
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    -ms-flex-negative: 1;
    flex-shrink: 1;
}

.flex-x-center {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}

.flex-y-center {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -ms-grid-row-align: center;
    align-items: center;
}

.flex-y-bottom {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: end;
    -ms-flex-align: end;
    -ms-grid-row-align: flex-end;
    align-items: flex-end;
}